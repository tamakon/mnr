{
  "name": "mnr",
  "version": "1.0.0",
  "description": "いい感じの企業ホームページを作ろう！",
  "dependencies": {
    "ip": "1.1.5",
    "express": "3.0.5",
    "vue": "2.5.2",
    "browserify": "14.4.0",
    "babelify": "7.3.0",
    "vueify": "9.4.1",
    "babel-preset-es2015": "6.24.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "concurrently": "3.5.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": "es2015"
        }
      ],
      "vueify"
    ]
  },
  "scripts": {
    "build": "browserify src/js/index.js -o dist/js/index.js",
    "heroku": "concurrently \"npm run build\" \"node main.js\" "
  }
}