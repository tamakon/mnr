{
  "name": "mnr",
  "version": "1.0.0",
  "description": "いい感じの企業ホームページを作ろう！",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/tamakon/mnr.git"
  },
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babelify": "7.3.0",
    "browserify": "14.4.0",
    "concurrently": "3.5.0",
    "express": "3.0.5",
    "ip": "1.1.5",
    "vue": "^2.5.8",
    "vue-router": "3.0.1",
    "vueify": "^9.4.1",
    "watchify": "^3.9.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": "es2015"
        }
      ],
      "vueify"
    ]
  },
  "scripts": {
    "build": "browserify src/js/index.js -o dist/js/index.js",
